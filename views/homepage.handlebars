<h3>Here are your cryptocurrency holdings.</h3>
<table border="border: 1px solid #333">
    <thead>
        <tr>
            <th>Crypto Name</th>
            <th>Amount</th>
            <th>Price</th>
            <th>Value</th>
            <th>Cost</th>
            <th>Profit</th>
        </tr>
    </thead>
    <tbody>
        {{#each cryptoholdings}}
        <tr>
            <td>{{this.crypto_name}}</td>
            <td>{{this.crypto_amount}}</td>
            <td>{{this.crypto_price}}</td>
            <td>{{this.crypto_value}}</td>
            <td>{{this.crypto_cost}}</td>
            <td>{{this.crypto_profit}}</td>
        </tr>
        {{/each}}
    </tbody>

</table>



<h3>Crypto Prices</h3>
<table border="border: 1px solid #333">
    <thead>
        <tr>
            <th>Crypto</th>
            <th>price</th>

        </tr>
    </thead>
    <tbody>
        {{#each all_crypto}}
        <tr>
            <td>{{this.crypto_name}}</td>
            <td>{{this.crypto_price}}</td>

        </tr>
        {{/each}}
    </tbody>

</table>



